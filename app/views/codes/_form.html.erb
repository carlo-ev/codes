<%= form_for @code do |f| %>
	<div class="field">
		<%= f.label :title%>
		<%= f.text_field :title %>
	</div>
	<div class="field">
		<%= f.label :description %>
		<%= f.text_area :description%>
	</div>
	<div class="field">
		<%= f.label :content %>
		<%= f.text_area :content %>
	</div>
	<div class="actions"><%= f.submit %></div>
<% end %>
